{
  "name": "mars-date-utils",
  "version": "0.1",
  "description": "JavaScript Library for working with time on Mars",
  "main": "dist/index.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dev": "nodemon ./dist/test.js --ignore 'dist/**' -e ts --exec \"npm run compile\"",
    "compile": "npm run clear && tsc && node ./dist/test.js",
    "build": "tsc",
    "start": "node ./dist/test.js",
    "prepublish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendahu/mars-date.git"
  },
  "author": "Jake Robins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mendahu/mars-date/issues"
  },
  "homepage": "https://github.com/mendahu/mars-date#readme",
  "devDependencies": {
    "@types/node": "^14.14.37",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "types": "dist/index.d.ts"
}
